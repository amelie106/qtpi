{
  "expo": {
    "name": "QTpi",
    "description": "My Medical Calculator App",
    "slug": "QTpi",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./images/qtAppIcon.png",
    "splash": {
      "image": "./images/qtAppIcon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.univie.QTpi"
    }
  }
}
